package com.java8.prctc;

interface Cal{  // this is functional interface which contains only single abstarct method
	public int add(int a,int b);
}
public class Java8eg {

	public static void main(String[] args) {
	   Cal c=(a,b)->{//lambda function  --> store in interface class type,
		 return a+b;  
	   };
	   System.out.println("sum is "+c.add(14,20));
	}

}
